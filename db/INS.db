CREATE TABLE Subjects (
    id_subject INT (2) AUTO_INCREMENT,
    name_subject_arabic VARCHAR (45) NOT NULL,
    id_year INT (1) NOT NULL,
    id_sem INT (1) NOT NULL,
    ac_date date NOT NULL,
    id_time INT (1) NOT NULL,
    id_user INT (11) NOT NULL,
  	CONSTRAINT PK_id_subject PRIMARY KEY (id_subject),
    CONSTRAINT FK_id_year FOREIGN KEY (id_year) REFERENCES years(id_year),
    CONSTRAINT FK_id_time FOREIGN KEY (id_time) REFERENCES Times(id_time),
    CONSTRAINT FK_id_user FOREIGN KEY (id_user) REFERENCES Users(id_user)
);

