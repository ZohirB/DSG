CREATE TABLE Users (
  id_user INT (2) AUTO_INCREMENT,
  user_name varchar(255) NOT NULL,
  password varchar(255) NOT NULL,
  name varchar(255) NOT NULL,
  college_name varchar(255) NOT NULL,
  CONSTRAINT PK_id_user PRIMARY KEY (id_user)
);
