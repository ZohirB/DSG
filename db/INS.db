CREATE TABLE years (
    id_year INT (1) AUTO_INCREMENT,
    name_year_english VARCHAR (25) NOT NULL UNIQUE,
    name_year_arabic VARCHAR (25) NOT NULL UNIQUE,
  	CONSTRAINT PK_id_year PRIMARY KEY (id_year)
);
